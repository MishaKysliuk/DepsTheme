@font-face
	font-family: "svgfont"
	src: url('./../css/svgfont.eot')
	src: url('./../css/svgfont.eot?#iefix') format('embedded-opentype'), 
			 url('./../css/svgfont.woff') format('woff'),
			 url('./../css/svgfont.woff2') format('woff2'),
			 url('./../css/svgfont.ttf') format('truetype')
	font-weight: normal
	font-style: normal
	font-display: swap

